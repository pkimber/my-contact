    "@ember-data/json-api": "5.4.1",
    "@ember-data/request": "5.4.1",
    "@ember-data/request-utils": "5.4.1",
    "@ember-data/store": "5.4.1",
    "@ember-data/tracking": "5.4.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "5.2.1",
    "@embroider/compat": "^4.0.0-alpha.0",
    "@embroider/config-meta-loader": "^1.0.0-alpha.0",
    "@embroider/core": "^4.0.0-alpha.0",
    "@embroider/macros": "^1.16.12",
    "@embroider/vite": "^1.0.0-alpha.0",
    "@eslint/js": "^9.23.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@warp-drive/build-config": "5.4.1",
    "@warp-drive/core-types": "5.4.1",
    "@warp-drive/ember": "5.4.1",
